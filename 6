#include <stdio.h>

int main() {
    char ch;
    printf("enter a string:");
    scanf("%c",&ch);
    if (ch>='A'&& ch<='Z')
    {
        printf("'%c'is a uppercase");
    }
    else if(ch>='a'&& ch<='z')
    {
        printf("'%c'is a lowercase");
    }
    else
    {
        printf("'%c'is not a alphabet");
    }
}
